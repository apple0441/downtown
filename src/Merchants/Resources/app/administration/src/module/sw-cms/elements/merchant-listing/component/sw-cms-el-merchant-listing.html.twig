{% block sw_cms_element_merchant_listing %}
    <div class="sw-cms-el-merchant-listing">
        <div class="sw-cms-el-merchant-listing__content">
            <sw-cms-merchant-box-preview v-for="index in demoMerchantCount"></sw-cms-merchant-box-preview>
        </div>

        <div class="sw-cms-el-merchant-listing__pagination">
            <div class="sw-cms-el-merchant-listing__pagination-entry">
                <sw-icon name="small-arrow-small-left" size="20px"></sw-icon>
            </div>

            <div class="sw-cms-el-merchant-listing__pagination-entry">1</div>
            <div class="sw-cms-el-merchant-listing__pagination-entry">2</div>
            <div class="sw-cms-el-merchant-listing__pagination-entry">3</div>

            <div class="sw-cms-el-merchant-listing__pagination-entry">
                <sw-icon name="small-arrow-small-right" size="20px"></sw-icon>
            </div>
        </div>
    </div>
{% endblock %}
{##}
